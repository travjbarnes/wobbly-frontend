{
  "expo": {
    "name": "Wobbly",
    "slug": "wobbly-dev",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.1-alpha.1",
    "orientation": "portrait",
    "primaryColor": "#ed2826",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ed2826"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "android": {
      "package": "app.wobbly.frontend",
      "versionCode": 2
    },
    "ios": {
      "supportsTablet": false
    },
    "description": "Wobbly is a workplace organising platform. Weâ€™re creating a space for energetic, powerful, and democratic unions to win struggles and grow.",
    "githubUrl": "https://github.com/Wobbly-App/wobbly-frontend",
    "extra": {
      "backendUrl": "http://develop.wobbly.app"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "wobbly",
            "project": "wobbly-frontend",
            "authToken": ""
          }
        }
      ]
    }
  }
}